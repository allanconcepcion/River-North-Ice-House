{
  "name": "headless-template",
  "version": "1.0",
  "private": false,
  "scripts": {
    "dev": "faust dev",
    "build": "faust build && npm run sitemap",
    "lint": "faust lint",
    "generate": "faust generatePossibleTypes && npm run codegen",
    "clean": "rimraf .next node_modules",
    "start": "faust start",
    "codegen": "graphql-codegen --config graphql.config.yaml --errors-only",
    "format": "prettier --write './**/*.{js,jsx,md,mdx,css,scss,ts,tsx}' '!.next/**' '!out/**' '!possibleTypes.json' '!public/~partytown/**'",
    "format:check": "prettier --check './**/*.{js,jsx,md,mdx,css,scss,ts,tsx}'",
    "sitemap": "next-sitemap"
  },
  "dependencies": {
    "@apollo/client": "^3.7.11",
    "@faustwp/cli": "0.2.7",
    "@faustwp/core": "0.2.8",
    "@stripe/stripe-js": "^3.3.0",
    "@vercel/analytics": "^1.3.1",
    "@vercel/og": "^0.6.2",
    "clsx": "^2.1.1",
    "framer-motion": "^11.1.7",
    "graphql": "^16.6.0",
    "next": "^14.2.3",
    "next-seo": "^6.0.0",
    "next-sitemap": "^4.0.6",
    "react": "^18.2.0",
    "react-countup": "^6.5.3",
    "react-dom": "^18.2.0",
    "react-icons": "^5.1.0",
    "react-inlinesvg": "^3.0.3",
    "react-lite-youtube-embed": "^2.3.52",
    "react-responsive-carousel": "^3.2.23",
    "react-select": "^5.7.2",
    "react-toastify": "^10.0.5",
    "react-wrap-balancer": "^1.1.0",
    "stripe": "^15.3.0",
    "tailwind-merge": "^2.3.0",
    "typescript": "5.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/eslint-parser": "^7.21.3",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript-operations": "^4.2.0",
    "@graphql-codegen/typescript-react-apollo": "^4.3.0",
    "@next/bundle-analyzer": "^14.2.3",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/forms": "^0.5.3",
    "@types/graphql": "^14.5.0",
    "@types/node": "^20.12.7",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "apollo": "^2.34.0",
    "autoprefixer": "^10.4.14",
    "bundlesize": "^0.18.1",
    "css-loader": "^7.1.1",
    "dotenv-flow": "^4.1.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "^14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "fs": "^0.0.1-security",
    "isomorphic-fetch": "^3.0.0",
    "mini-css-extract-plugin": "^2.7.5",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "replace": "^1.2.2",
    "rimraf": "^5.0.5",
